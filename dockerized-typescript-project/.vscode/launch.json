  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to container",
      "type": "node",
      "request": "attach",
      "port": 9229,
      "address": "localhost",
      "localRoot": "${workspaceFolder}/example-web-server/src",
      "remoteRoot": "/home/node/app",
      "protocol": "inspector",
      "restart": true,
      "skipFiles": ["<node_internals>/**"],
      "sourceMapPathOverrides": {
        "/home/node/app*": "${workspaceRoot}/example-web-server/*"
      }
    }
  ]
}
